          "rawQuery": true,
          "rawSql": "-- Host Health: Per-host metrics with approval and success rates\nSELECT\n  host_id,\n  COUNT(*) as total_events,\n  COUNT(DISTINCT session_id) as sessions,\n  SUM(CASE WHEN decision = 'DENY' THEN 1 ELSE 0 END) as denials,\n  SUM(CASE WHEN decision = 'ERROR' THEN 1 ELSE 0 END) as errors,\n  MAX(created_at) as last_seen,\n  ROUND(100.0 * SUM(CASE WHEN decision IN ('APPROVE','CONFIRM','SUCCESS') THEN 1 ELSE 0 END) / NULLIF(COUNT(*), 0), 1) as approval_rate,\n  ROUND(100.0 * SUM(CASE WHEN decision != 'ERROR' THEN 1 ELSE 0 END) / NULLIF(COUNT(*), 0), 1) as success_rate\nFROM telemetry_events\nWHERE created_at > NOW() - INTERVAL '24 hours'\nGROUP BY host_id\nORDER BY total_events DESC",
